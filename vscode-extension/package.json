{
  "name": "mimo-assembler",
  "displayName": "MImo Assembler",
  "description": "Adds support for mia files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "mia",
      "aliases": ["mia", "mia"],
      "extensions": [".mia"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mia",
      "scopeName": "source.mia",
      "path": "./syntaxes/mia.tmLanguage.json"
    }]
  }
}
